includes:
    - ./vendor/nunomaduro/larastan/extension.neon

parameters:

    paths:
        - app

    # The level 8 is the highest level
    level: 7

    excludePaths:
        - ./app/*/Kernel.php
        - ./app/Http/Middleware/*
        - ./app/Providers/*
        - ./app/Exceptions/*
        - ./app/Models/User.php
